{
  "name": "shrine",
  "version": "0.0.1",
  "description": "ballin like LaMelo Ball",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "electron": "^25.0.0",
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-dmg": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2"
  },
  "author": "titanbound",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "shrine"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO",
            "name": "shrine-${version}"
          }
        }
      ]
    }
  }
}